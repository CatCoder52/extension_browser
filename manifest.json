{
  "name": "Privacy Redirect",
  "description": "Redirects Twitter & Youtube requests to privacy friendly alternatives (Nitter & Invidious).",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://twitter.com/*",
    "*://www.twitter.com/*",
    "*://mobile.twitter.com/*",
    "*://youtube.com/*",
    "*://www.youtube.com/*",
    "*://youtube-nocookie.com/*",
    "*://www.youtube-nocookie.com/*",
    "*://m.youtube.com/"
  ]
}